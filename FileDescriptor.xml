<assembly xmlns="http://maven.apache.org/POM/4.0.0" 
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  			xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">

	<id>assembly-id</id>

	<formats>
		<format>zip</format>
	</formats>
	
	
	<fileSets>
	    <fileSet>
	      <directory>${project.basedir}\Input_File</directory>
	      <outputDirectory></outputDirectory>
	      <includes>
	        <include>Business_Glossary/*.csv</include>
	        <include>Informatica_PWC_XML/*.xml</include>
	       	<include>Shell_Script/*.sh</include>
	        <include>Tableau_XML/*.xml</include>
	      </includes>
	    </fileSet>

	</fileSets>
 
</assembly>